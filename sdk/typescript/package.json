{
  "name": "@edgeai/sdk",
  "version": "0.1.0",
  "description": "Official JavaScript/TypeScript SDK for EdgeAI Blockchain",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "lint": "eslint src --ext .ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "edgeai",
    "blockchain",
    "sdk",
    "web3",
    "iot",
    "edge-computing",
    "proof-of-information-entropy"
  ],
  "author": "EdgeAI Foundation",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Free0x/edgeai-alpha.git",
    "directory": "sdk/typescript"
  },
  "homepage": "https://edgeai.network",
  "bugs": {
    "url": "https://github.com/Free0x/edgeai-alpha/issues"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "eventemitter3": "^5.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "eslint": "^8.54.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.2",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
